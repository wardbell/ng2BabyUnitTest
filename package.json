{
  "name": "ng2BabyUnitTest",
  "version": "1.0.0",
  "description": "Angular 2 Baby Unit Testing",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run tsd",
    "tsd": "tsd link --config src/tsd.json && tsd reinstall -ro --config src/tsd.json",
    "tsc": "tsc -p src -w",
    "start": "live-server --open=src",
    "test": "live-server --open=src/unit-tests.html"
  },
  "dependencies": {
    "angular2": "2.0.0-alpha.38",
    "systemjs": "0.19.2"
  },
  "devDependencies": {
    "jasmine-core": "^2.3.4",
    "live-server": "0.8.1",
    "tsd": "0.6.5",
    "typescript": "1.6.2",
    "zone.js": "^0.5.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wardbell/ng2BabyUnitTest.git"
  },
