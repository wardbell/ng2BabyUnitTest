{
  "name": "ng2BabyUnitTest",
  "description": "Angular 2 Baby Unit Testing",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "npm run tsd",
    "tsd": "tsd link --config src/tsd.json && tsd reinstall -r -o --config src/tsd.json",
    "tsc": "tsc -p src -w",
    "start": "live-server --open=src",
    "test": "live-server --open=src/unit-tests.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wardbell/ng2BabyUnitTest"
  },
  "licenses": [
    {
      "type": "Apache",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "dependencies": {
    "angular2": "2.0.0-alpha.37",
    "systemjs": "0.19.0",
    "traceur": "0.0.91"
  },
  "devDependencies": {
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.8",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "zone.js": "^0.5.3"
  },
  "contributors": [
    "Ward Bell <wardb@ideablade.com>"
  ]
}